  },
  "devDependencies": {
    "@tanstack/react-query": "^5.81.2",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.3",
    "@vitest/coverage-v8": "^1.6.0",
    "dotenv": "^17.2.2",
    "jsdom": "^24.1.0",
    "react": ">=18.0.0",
    "tsup": "^8.5.0",
